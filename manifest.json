{
    "name":"Productive Pomodoro",
    "version": "0.0.1",
    "manifest_version": 3,
    "description": "Pomodoro Timer for increasing productivity",
    "content_scripts":[
        {   
            "matches":["<all_urls>"],
            "js": ["script.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon" : "logo.png"
    },
    "icons": {
        "128": "logo.png"
    },
    "permissions": ["activeTab"]
}